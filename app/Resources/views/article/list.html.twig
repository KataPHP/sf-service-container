{% extends 'base.html.twig' %}

{% block body %}

    <h1>List articles</h1>

    <table>
        <tbody>
        {% for article in articles %}
            <tr>
                <td>{{ article.id }}</td>
                <td>{{ article.title|MyUppercase }}</td>
                <td>{{ article.body }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}